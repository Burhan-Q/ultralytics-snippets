{
	"Ultralytics YOLO Predict Once": {
		"prefix": "ultra.yolo.predict",
		"body": [
			"from ultralytics import YOLO",
			"",
			"model = YOLO(\"${1:yolov8n.pt}\", task=\"${2:detect}\")",
			"results: list = model.predict(source=${3:None})"
		],
		"description": "Use Ultralytics YOLO to perform inference once a single source."
	},

	"Ultralytics SAM Predict Once": {
		"prefix": "ultra.sam.predict",
		"body": [
			"from ultralytics import SAM",
			"",
			"model = SAM(\"${1:sam.pt}\", task=\"${2:detect}\")",
			"results: list = model.predict(source=$3)"
		],
		"description": "Use Ultralytics SAM to perform inference once a single source."
	}
}
